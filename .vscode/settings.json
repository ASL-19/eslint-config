{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "cSpell.words": [
    "Heaslip",
    "Immer",
    "destructure",
    "mixins",
    "rushstack",
    "tsdoc"
  ],
  "markdown.extension.toc.levels": "2..6",
  "search.exclude": {
    "CHANGELOG.markdown": true,
    "LICENSE": true,
    "package-lock.json": true
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
